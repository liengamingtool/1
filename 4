echo "$res  \r";
}}

function delay2($delaybl){
  $green="\e[1;32m";
  $yellow="\e[1;33m";
  for($j = $delaybl;$j> 0;$j--){
    echo "\r                                                      \r";
       echo "\r\033[1;95mƒêang Ngh·ªâ Chu·ªëng Block L√†m Ti·∫øp Sau:\033[1;93m $j\033[1;93m Gi√¢y ";
usleep(133333);
